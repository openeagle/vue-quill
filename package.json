{
  "name": "@openeagle/vue-quill",
  "version": "1.0.0",
  "license": "MIT",
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "sideEffects": false,
  "files": [
    "dist",
    "es",
    "lib"
  ],
  "scripts": {
    "serve": "vue-cli-service serve ./examples/main.ts",
    "build": "npm run build:dist && npm run compile:commojs && npm run compile:esmodule",
    "build:dist": "vue-cli-service build --target lib ./src/index.ts",
    "build:report": "vue-cli-service build --report --target lib ./src/index.ts",
    "compile:commojs": "rimraf ./lib && babel --env-name commojs -x .ts,.tsx ./src -d ./lib",
    "compile:esmodule": "rimraf ./es && babel --env-name esmodule -x .ts,.tsx ./src -d ./es",
    "lint": "vue-cli-service lint",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prerelease": "npm run build",
    "release": "semantic-release"
  },
  "dependencies": {
    "@babel/runtime": "^7.1.1",
    "core-js": "^3.0.0",
    "quill": "^1.0.0",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@storybook/addon-actions": "^6.2.0-beta.10",
    "@storybook/addon-essentials": "^6.2.0-beta.10",
    "@storybook/addon-links": "^6.2.0-beta.10",
    "@storybook/vue3": "^6.2.0-beta.10",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "@vue/cli-plugin-babel": "^4.5.0",
    "@vue/cli-plugin-eslint": "^4.5.0",
    "@vue/cli-plugin-typescript": "^4.5.0",
    "@vue/cli-service": "^4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "babel-loader": "^8.2.2",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^7.0.0-0",
    "less": "^4.1.1",
    "less-loader": "^7.3.0",
    "postcss": "^8.2.6",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.4.1",
    "typescript": "^4.2.2",
    "vue-loader": "^16.1.2",
    "vue-template-compiler": "^2.6.12"
  }
}
